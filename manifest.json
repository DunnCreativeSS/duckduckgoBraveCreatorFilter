{
      "name": "DuckDuckGo Brave Publisher Filter",
      "description": "Filter your DuckDuckGo search results by Brave Publishers",
      "omnibox": {
        "keyword": "duckduckgo",
		"keyword": "Brave",
		"keyword": "BAT"
          },
      "icons": {
        "16": "icon.png"
      },
      "background": {
        "scripts": ["data.js", "jquery.js", "background.js"]
      },
	  "permissions": [
			"tabs",
			"http://*/*"
		],
      "version": "1.0",
      "minimum_chrome_version": "9",
      "manifest_version": 2
    }
  